<panel
 follows="all"
 height="768"
 layout="topleft"
 name="panel_login"
 focus_root="true"
 background_visible="true"
 bg_opaque_color="0.1372549019607843 0.1372549019607843 0.1372549019607843 1"
 background_opaque="true"
 width="1024">
 <layout_stack
 follows="left|right|top"
 height="500"
 left="0"
 name="ui_stack"
 orientation="horizontal"
 top="0"
 width="1024">
    <layout_panel
    height="172"
    auto_resize="false"
    name="ui_elastic_pad_left"
    width="5" />
    <layout_panel
    auto_resize="false"
    follows="left|right|top"
    name="ui_container"
    width="960"
    left="0"
    top="0"
    height="172">
 <combo_box
	left="1"
	top="50"
	allow_text_entry="true"
	follows="left|top"
	height="25"
	label="Username"
	combo_editor.font="SansSerif"
	max_chars="128"
	combo_editor.commit_on_focus_lost="false"
	combo_editor.prevalidate_callback="ascii"
	tool_tip="The username you chose when you registered, like bobsmith12 or Steller Sunshine"
	name="username_combo"
	width="233">
	<combo_box.combo_editor
	text_pad_left="8" />
	<combo_box.combo_button
	visible="true" />
	<combo_box.drop_down_button
	visible="true" />
 </combo_box>
 <line_editor
	follows="left|top"
	font="SansSerif"
	height="25"
	is_password="true"
	label="Password"
	left_delta="0"
	max_length_bytes="16"
	name="password_edit"
	select_on_focus="true"
	text_pad_left="8"
	top_pad="10"
	width="232" />
 <button
    follows="left|top"
    image_unselected="PushButton_Off"
    image_pressed="PushButton_Selected_Press"
    label="Credentials&#63;"
    label_color="White"
    font="SansSerif"
    name="connect_btn"
    enabled="false"
    left="1"
    top_pad="10"
    width="110"
    height="26"/>
 <check_box
    control_name="RememberPassword"
    follows="left|top"
    font="SansSerif"
    left_pad="5"
    top_delta="-1"
    height="25"
    label="Remember me"
    check_button.bottom="3"
    name="remember_check"
    width="145" />
 <combo_box
    allow_text_entry="true"
	control_name="NextLoginLocation"
	follows="left|top"
	label="Start location"
	height="26"
	max_chars="128"
	combo_editor.font="SansSerif"
	left="1"
	top_pad="10"
	name="start_location_combo"
	width="232"
	combo_button.scale_image="true">
	<combo_box.item
	label="Last or Custom Location"
	name="Last"
	value="last" />
	<combo_box.item
	label="My home"
	name="MyHome"
	value="home" />
 </combo_box>
 <combo_box
    allow_text_entry="false"
    follows="left|top"
    image_pressed="PushButton_Press"
    image_hover="PushButton_Over"
    image_hover_unselected="PushButton_Off"
    combo_editor.font="SansSerif"
    height="26"
    label="Grid selection"
    layout="topleft"
    left="1"
    max_chars="128"
    name="server_combo"
    top_pad="11"
    width="232" />
</layout_panel>
    <layout_panel
    height="172"
    auto_resize="true"
    name="ui_elastic_pad_right"
    width="32" />
 </layout_stack>
 <web_browser
    background_opaque="true"
    bg_opaque_color="0.1372549019607843 0.1372549019607843 0.1372549019607843 1"
    tab_stop="false"
    trusted_content="true"
    border_visible="false"
    follows="all"
    left="250"
    name="login_html"
    start_url=""
    top="18"
    height="750"
    width="774" />
</panel>
