<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 legacy_header_height="18"
 can_resize="true"
 height="250"
 layout="topleft"
 name="queue"
 help_topic="script_queue"
 save_rect="true"
 title="Reset Progress"
 width="300">
    <floater.string
     name="Starting">
        Starting [START] of [COUNT] items.
    </floater.string>
    <floater.string
     name="Done">
        Done.
    </floater.string>
    <floater.string
     name="Resetting">
        Resetting
    </floater.string>
    <floater.string
     name="Running">
        Running
    </floater.string>
    <floater.string
     name="NotRunning">
        Not running
    </floater.string>
    <floater.string
     name="Timeout">
        Timeout: [OBJECT_NAME]
    </floater.string>
    <floater.string
     name="LoadingObjInv">
        Loading inventory for: [OBJECT_NAME]
    </floater.string>
    <floater.string
        name="Deleting">
      Deleting
    </floater.string>
    <floater.string
        name="SkippingPermissions">
      Skipping: [OBJECT_NAME] (Permissions)
    </floater.string>
    <floater.string
        name="Compiling">
      Compiling "[OBJECT_NAME]"...
    </floater.string>
    <floater.string
        name="CompileSuccess">
      Compilation of "[OBJECT_NAME]" succeeded
    </floater.string>
    <floater.string
        name="CompileFailure">
      Compilation of "[OBJECT_NAME]" failed:
    </floater.string>
    <scroll_list
     follows="left|top|right|bottom"
     draw_stripes="true"
     layout="topleft"
     left="4"
     height="204"
     name="queue output"
     top_pad="20"
     width="292" />
    <check_box
     control_name="PVUI_ScriptQueueAutoClose"
     follows="left|bottom"
     height="20"
     layout="topleft"
     width="64"
     left="2"
     top_pad="3"
     label="Auto-close when done"
     name="auto_close_checkbox">
    </check_box>
    <button
     follows="right|bottom"
     height="20"
     label="Close"
     label_selected="Close"
     layout="topleft"
     right="-3"
     name="close"
     top_delta="0"
     width="64" />
</floater>
